<header class="c-header">
  <div class="l-container">
    <div class="c-header__extralink">
      <a href="#">掲載・取材依頼の企業様へ</a>
      <img class="c-header__arrowicon" src="./assets/img/arrow.svg" alt="掲載・取材依頼の企業様へ">
    </div>
    <div class="c-header__main">
      <div class="c-header__logo"><a href="index.html"><img src="/assets/img/logo.png" alt="Home page"></a></div>
      <nav class="c-header__mainnav">
        <ul class="c-header__navlinks">
          <% if (typeof linklists != 'undefined') { %>
            <% linklists.main && linklists.main.forEach(link => { %>
              <li class="c-header__navlink">
                <a <% if (link.current || link.active) { %>class="is-active"<% } %> href="<%= link.url %>"><%= link.title %></a>
                <% if (link.links) { %>
                  <ul>
                    <% link.links.forEach(child_link => { %>
                      <li>
                        <a <% if (child_link.current) { %>class="is-active"<% } %> href="<%= child_link.url %>"><%= child_link.title %></a>
                      </li>
                    <% }) %>
                  </ul>
                <% } %>
              </li>
            <% }) %>
          <% } %>
        </ul>
      </nav>
      <a class="c-header__inquiry" href="#">お問い合わせ</a>
    </div>
  </div>
</header>
